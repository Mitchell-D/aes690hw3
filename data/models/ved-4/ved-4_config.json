{
    "model_name": "ved-4",
    "input_feats": [
        "tmpc",
        "dwpc",
        "relh",
        "sknt",
        "mslp",
        "p01m",
        "gust",
        "feel"
    ],
    "output_feats": [
        "romps_LCL_m",
        "lcl_estimate"
    ],
    "source_csv": "data/AL_ASOS_July_2023.csv",
    "batch_size": 32,
    "batch_buffer": 4,
    "dropout_rate": 0.0,
    "batchnorm": true,
    "dense_kwargs": {
        "activation": "sigmoid"
    },
    "enc_node_list": [
        128,
        128,
        128,
        64,
        32
    ],
    "latent_size": 8,
    "dec_node_list": [
        32,
        64,
        64,
        16
    ],
    "loss": "mse",
    "metrics": [
        "mse",
        "mae"
    ],
    "enc_dense_kwargs": {
        "activation": "relu"
    },
    "dec_dense_kwargs": {
        "activation": "relu"
    },
    "weighted_metrics": [
        "mae",
        "mse"
    ],
    "max_epochs": 128,
    "train_steps_per_epoch": 16,
    "val_steps_per_epoch": 3,
    "val_frequency": 1,
    "learning_rate": 0.01,
    "early_stop_metric": "val_mse",
    "early_stop_patience": 30,
    "mask_pct": 0,
    "mask_pct_stdev": 0,
    "mask_val": 9999.0,
    "train_val_ratio": 0.8,
    "mask_feat_probs": null,
    "save_weights_only": true,
    "notes": "Same as ved-3, but totally new class-based model!"
}